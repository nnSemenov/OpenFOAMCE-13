FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.water 0;
}

zones
{
    cell
    {
        type        box;

        box         (-999 -999 -999) (999 999 0.065);

        values
        {
            alpha.water 1;
        }
    }
}

extrapolatePatches
{
    "inlet|outlet"   (alpha.water);
}

// ************************************************************************* //
