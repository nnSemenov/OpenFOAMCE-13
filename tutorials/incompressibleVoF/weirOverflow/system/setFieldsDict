FoamFile
{
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultValues
{
    alpha.water 0;
}

zones
{
    water
    {
        type        box;

        box         (-100 0 -100) (0 20 100);

        values
        {
            alpha.water 1;
        }
    }
}

extrapolatePatches
{
    inlet       (alpha.water);
}

// ************************************************************************* //
